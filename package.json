{
    "name": "@nickkaramoff/rollup-plugin-consts",
    "version": "1.0.2",
    "description": "Import build time constants with Rollup",
    "main": "dist/rollup-plugin-consts.cjs.js",
    "module": "dist/rollup-plugin-consts.es.js",
    "types": "index.d.ts",
    "files": [
        "dist",
        "index.d.ts",
        "consts.d.ts"
    ],
    "scripts": {
        "build": "rollup -c",
        "pretest": "npm run build",
        "test": "jest",
        "format": "prettier \"src/*.js\" \"test/**/*.js\" \"*.{js,ts,md}\" --write"
    },
    "repository": "NickKaramoff/rollup-plugin-consts",
    "bugs": "https://github.com/NickKaramoff/rollup-plugin-consts/issues",
    "homepage": "https://github.com/NickKaramoff/rollup-plugin-consts#readme",
    "keywords": [
        "rollup",
        "rollup-plugin",
        "const",
        "consts",
        "constant"
    ],
    "author": "Jake Archibald",
    "contributors": [
        "Tiger Oakes <contact@tigeroakes.com> (https://tigeroakes.com)",
        "Nikita Karamov <nick@karamoff.dev> (https://karamoff.dev)"
    ],
    "license": "Apache-2.0",
    "peerDependencies": {
        "rollup": ">=1.15.0 <3"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.4.0",
        "@types/jest": "^25.2.1",
        "javascript-stringify": "^2.0.1",
        "jest": "^25.4.0",
        "prettier": "^2.0.5",
        "rollup": "^2.7.3",
        "typescript": "^3.8.3"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "es5",
        "proseWrap": "always"
    },
    "publishConfig": {
        "access": "public"
    }
}